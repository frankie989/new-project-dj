<%
const cfg = { ...require("./data.json"), ...arguments?.[0] }
const {
	extraClasses,
	extraAttrs,
	type,
	placeholder,
	alt,
	id,
	autocomplete,
	value,
	name

} = cfg
%>

<%
const classes = `input ${extraClasses.join(" ")}`.trim()
const attrs = extraAttrs.join(" ")
%>
<% if (type)  %>
<input
	class="<%= classes %>"
	<%- extraAttrs %>
	name = "<%= name%>"
	placeholder = "<%= placeholder%>"
	alt = "<%= alt%>"
	id = "<%= id%>"
	autocomplete = "<%= autocomplete%>"
	value = "<%= value%>"


>


